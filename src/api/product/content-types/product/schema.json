{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 15
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "countPlayers": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "slug": {
      "type": "customField",
      "options": {
        "pattern": "title"
      },
      "customField": "plugin::slug.slug"
    },
    "descrition": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "available": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "colorBox": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::color.color"
    },
    "additions": {
      "type": "boolean",
      "default": false
    },
    "videos": {
      "type": "component",
      "repeatable": true,
      "component": "product.videos"
    },
    "languages": {
      "type": "component",
      "repeatable": true,
      "component": "product.languages"
    },
    "manual": {
      "type": "component",
      "repeatable": true,
      "component": "product.manual"
    },
    "dicount": {
      "type": "integer",
      "max": 100,
      "default": 0
    }
  }
}
